{
    "for": {
        "prefix": "for",
        "body": [
            "{% for ${iterable} in ${dict} %}",
            "   {{ ${iterable} }}",
            "{% endfor %}"
        ],
        "description": "Returns a sequence of data table rows based on the query."
     },
     "if": {
        "prefix": "if",
        "body": [
            "{% if ${test} %}",
            "   ${do_something}",
            "{% endif %}"
        ],
        "description": "Returns a sequence of data table rows based on the query."
     },
     "elif": {
        "prefix": "elif",
        "body": [
            "{% elif ${test} %}"
        ],
        "description": "Else if statement to be used within if statement"
    },
    "with": {
        "prefix": "with",
        "body": [
            "{% with myVar=${test} %}",
            "    {{ ${myVar} }}",
            "{% endwith %}"
        ],
        "description": "Sets a variable"
    }
}